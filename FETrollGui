    local library = loadstring(game:HttpGet(('https://raw.githubusercontent.com/bloodball/-back-ups-for-libs/main/wall%20v3')))()
    
    local w = library:CreateWindow("FE1 Trolling GUI") -- Creates the window
    
    local b = w:CreateFolder("Troll") -- Creates the folder(U will put here your buttons,etc)
    
    local answer
    b:Dropdown("Win Zone",{"Easy","Medium","Hard"},true,function(value) --true/false, replaces the current title "Dropdown" with the option that t
        answer = value
        print(answer)
    end)

    b:Button("Win",function()
        if answer == "Hard" then
            hard()
        else
            easy()
        end
    end)

    b:Button("Buy taunt",function()
        buyTaunt()
    end)
    
    b:Button("Taunt",function()
        doTaunt()
    end)
    
    local Thinger
    b:Box("Custom ID","number",function(value) -- "number" or "string"
        Thinger = value
    end)
    
    local Itemery
    b:Dropdown("Item",{"Dash","FloodSword","Trophy","WeakGravCoil","FloodShard"},true,function(value) --true/false, replaces the current title "Dropdown" with the option that t
        Itemery = value
    end)
    
    b:Button("Grab Item",function()
        doItem(Itemery)
    end)
    
    b:Button("Respawn",function()
        doRespawn()
    end)
    
    b:DestroyGui()
    
    b:Label("Discord: ive seen sunlight#6040",{
        TextSize = 12; -- Self Explaining
        TextColor = Color3.fromRGB(3,252,140); -- Self Explaining
        BgColor = Color3.fromRGB(24,1,48); -- Self Explaining
        
    }) 
    
    -- script
    
    function doTaunt()
        task.spawn(function()
            workspace.svcl_com.Taunt:FireServer()
            print("taunted")
        end)
    end
    
    function buyTaunt()
        task.spawn(function()
    
            local args = {
                [1] = "http://www.roblox.com/asset/?id="..Thinger
            }
    
            workspace.svcl_com.ShopTaunt:FireServer(unpack(args))
    
        
        end)
    end
    local plr = game.Players.LocalPlayer.Character    
function easy()
if answer == "Easy" then
    for i,v in pairs(game.Workspace:GetDescendants()) do
	if v:IsA("Part") and v.Parent.Name == "Lift" and v.Parent.Parent.Name == "Easy" then
		plr.HumanoidRootPart.CFrame = v.CFrame
		wait()
	end
end
for i,v in pairs(game.Workspace:GetDescendants()) do
	if v:IsA("Part") and v.Parent.Parent.Name == "Easy" and v.Name == "Exit" then
		wait(0.5)
        plr.HumanoidRootPart.CFrame = v.CFrame
	end
end
elseif answer == "Medium" then
medium()
end
end



function medium()
if answer == "Medium" then
    for i,v in pairs(game.Workspace:GetDescendants()) do
        if v:IsA("Part") and v.Parent.Name == "Lift" and v.Parent.Parent.Name == "Medium" then
            plr.HumanoidRootPart.CFrame = v.CFrame
            wait()
        end
    end
    
    for i,v in pairs(game.Workspace:GetDescendants()) do
        if v:IsA("Part") and v.Parent.Parent.Name == "Medium" and v.Name == "Exit" then
            wait(0.5)
            plr.HumanoidRootPart.CFrame = v.CFrame
        end
    end
elseif answer == "Hard" then
    hard()
end
end

function hard()
if answer == "Hard" then
        for i,v in pairs(game.Workspace:GetDescendants()) do
            if v:IsA("Part") and v.Parent.Name == "Lift" and v.Parent.Parent.Name == "Hard" then
                plr.HumanoidRootPart.CFrame = v.CFrame
                wait()
            end
        end
        
        for i,v in pairs(game.Workspace:GetDescendants()) do
            if v:IsA("Part") and v.Parent.Parent.Name == "Hard" and v.Name == "Exit" then
                wait(0.5)
                plr.HumanoidRootPart.CFrame = v.CFrame
            end
        end
        end
    end

    function doItem()
        task.spawn(function()
    
            local args = {
                [1] = Itemery
            }
    
            workspace.svcl_com.ShopExtra:FireServer(unpack(args))
    
        end)
    end
    
    function doRespawn()
        task.spawn(function()
    
            workspace.svcl_com.Respawn:FireServer()
    
        end)
    end
