local library = loadstring(game:HttpGet(('https://raw.githubusercontent.com/AikaV3rm/UiLib/master/Lib.lua')))()

local w = library:CreateWindow("FE1 Trolling GUI") -- Creates the window

local b = w:CreateFolder("Troll") -- Creates the folder(U will put here your buttons,etc)

b:Button("Buy taunt",function()
    buyTaunt()
end)

b:Button("Taunt",function()
    doTaunt()
end)

local Thinger
b:Box("Custom ID","number",function(value) -- "number" or "string"
    Thinger = value
end)

b:DestroyGui()

-- script

function doTaunt()
    task.spawn(function()
        workspace.svcl_com.Taunt:FireServer()
        print("taunted")
    end)
end

function buyTaunt()
    task.spawn(function()

        local args = {
            [1] = "http://www.roblox.com/asset/?id="..Thinger
        }

        workspace.svcl_com.ShopTaunt:FireServer(unpack(args))

    
    end)
end
