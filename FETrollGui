local library = loadstring(game:HttpGet(('https://raw.githubusercontent.com/AikaV3rm/UiLib/master/Lib.lua')))()

local w = library:CreateWindow("FE1 Trolling GUI") -- Creates the window

local b = w:CreateFolder("Troll") -- Creates the folder(U will put here your buttons,etc)

b:Button("Buy taunt",function()
    buyTaunt()
end)

b:Button("Taunt",function()
    doTaunt()
end)

local Thinger
b:Box("Custom ID","number",function(value) -- "number" or "string"
    Thinger = value
end)

local Itemery
b:Dropdown("Item",{"Dash","FloodSword","Trophy","WeakGravCoil","FloodShard"},true,function(value) --true/false, replaces the current title "Dropdown" with the option that t
    Itemery = value
end)

b:Button("Grab Item",function()
    doItem(Itemery)
end)

b:Button("Respawn",function()
    doRespawn()
end)

b:DestroyGui()

-- script

function doTaunt()
    task.spawn(function()
        workspace.svcl_com.Taunt:FireServer()
        print("taunted")
    end)
end

function buyTaunt()
    task.spawn(function()

        local args = {
            [1] = "http://www.roblox.com/asset/?id="..Thinger
        }

        workspace.svcl_com.ShopTaunt:FireServer(unpack(args))

    
    end)
end

function doItem()
    task.spawn(function()

        local args = {
            [1] = Itemery
        }

        workspace.svcl_com.ShopExtra:FireServer(unpack(args))

    end)
end

function doRespawn()
    task.spawn(function()

        workspace.svcl_com.Respawn:FireServer()

    end)
end
