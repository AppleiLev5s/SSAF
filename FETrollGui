if game.PlaceId == 32990482 then
	
    local library = loadstring(game:HttpGet(('https://raw.githubusercontent.com/bloodball/-back-ups-for-libs/main/wall%20v3')))()
    
    local w = library:CreateWindow("FE1 Trolling") -- Creates the window
    
    local b = w:CreateFolder("Folder") -- Creates the folder(U will put here your buttons,etc)
    
    b:Button("Buy taunt",function()
        buyTaunt()
    end)
    
    b:Button("Taunt",function()
        doTaunt()
    end)
    
    local Thinger
    b:Box("Custom ID","number",function(value) -- "number" or "string"
        Thinger = value
    end)
    
    local Itemery
    b:Dropdown("Item",{"Dash","FloodSword","Trophy","WeakGravCoil","FloodShard"},true,function(value) --true/false, replaces the current title "Dropdown" with the option that t
        Itemery = value
    end)
    
    b:Button("Grab Item",function()
        doItem(Itemery)
    end)
    
    b:Button("Respawn",function()
        doRespawn()
    end)
    
    b:DestroyGui()
    
    b:Label("Discord: ive seen sunlight#6040",{
        TextSize = 12; -- Self Explaining
        TextColor = Color3.fromRGB(3,252,140);
        BgColor = Color3.fromRGB(24,1,48);
        
    }) 
    
    -- script
    
    function doTaunt()
        task.spawn(function()
            workspace.svcl_com.Taunt:FireServer()
            print("taunted")
        end)
    end
    
    function buyTaunt()
        task.spawn(function()
    
            local args = {
                [1] = "http://www.roblox.com/asset/?id="..Thinger
            }
    
            workspace.svcl_com.ShopTaunt:FireServer(unpack(args))
    
        
        end)
    end
    
    function doItem()
        task.spawn(function()
    
            local args = {
                [1] = Itemery
            }
    
            workspace.svcl_com.ShopExtra:FireServer(unpack(args))
    
        end)
    end
    
    function doRespawn()
        task.spawn(function()
    
            workspace.svcl_com.Respawn:FireServer()
    
        end)
    end
    elseif not
        loadstring(game:HttpGet('https://raw.githubusercontent.com/MRMANSCRIpt/SSAF/main/Idiot'))() then
        print("This game isn't Flood Escape 1...")
    end
